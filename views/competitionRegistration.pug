<!DOCTYPE html>
<!-- Created By CodingLab - www.codinglabweb.com -->
<html lang="en" dir="ltr">
  <head>
    <meta charset="UTF-8">
    <title>Registration of Competition</title>
    <link rel="stylesheet" href="/CSS/competitionRegistration.css">
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
<body>
  <div class="container">
    <div class="title">Yoga Registration Form</div>
    <div class="content">
      <form id="competitionRegistrationForm">
        <div class="user-details">
          <div class="input-box">
            <span class="details">YSS ID</span>
            <input type="text" id="YSS_ID" placeholder="Enter your YSS ID" required>
          </div>
          <div class="input-box">
            <span class="details">Full Name</span>
            <input type="text" id="fullName" placeholder="Enter your Full Name" required>
          </div>
          <div class="input-box">
            <span class="details">Organisation Name</span>
            <input type="text" id="organisationName" placeholder="Enter organisation name"  required >
          </div>
          <div class="input-box">
            <span class="details">Date Of Birth</span>
            <input type="date" id="dob" placeholder="Enter your password" required>
          </div>
          <div class="input-box">
            <span class="details">Group</span>
            <input type="text" id="GroupNameInput" placeholder="Enter your Group" required readonly>
          </div>
          <div class="input-box" style="visibility:hidden">
            <span class="" id="cutOffDate">#{CutOffDate}</span>
            <span class="" id="YSS_Competition_ID">#{YSS_ID}</span>
            <span class="" id="Organiser">#{Organiser}</span>
          </div>
        </div>
        <div class="gender-details">
          <input type="radio" name="gender" value="Male" id="dot-1" required>
          <input type="radio" name="gender" id="dot-2" value="Female" required>
          <span class="gender-title">Gender</span>
          <div class="category">
            <label for="dot-1">
            <span class="dot one"></span>
            <span class="gender">Male</span>
          </label>
          <label for="dot-2">
            <span class="dot two"></span>
            <span class="gender">Female</span>
          </label>
          </div>
        </div>
        <div class="button">
          <button type="submit" id="competitionRegistrationBtn">Register</button>
        </div>
      </form>
    </div>
  </div>

  <script src="/JS/competitionRegistration.js"></script>

</body>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
</html>